<div id="rr-omni" class="rr-omni-int rr-modal-parent">
  <div class="rr-modal-backdrop"></div>
  <div id="rr-omni-custom" class="rr-modal-base">
    <div class="rr-modal-header">
      <div class="rr-back rr-back-hidden">
        <svg xmlns="http://www.w3.org/2000/svg" height="24" viewBox="0 0 24 24" width="24"><path d="M0 0h24v24H0V0z" fill="none"/><path d="M15.41 16.59L10.83 12l4.58-4.59L14 6l-6 6 6 6 1.41-1.41z"/></svg>
      </div>
      <h1 class="rr-modal-title">
        {{> skeleton height="18px" width="50%" }}
      </h1>
      <div class="rr-modal-close">
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="black" width="18px" height="18px"><path d="M0 0h24v24H0z" fill="none"/><path d="M19 6.41L17.59 5 12 10.59 6.41 5 5 6.41 10.59 12 5 17.59 6.41 19 12 13.41 17.59 19 19 17.59 13.41 12z"/></svg>
      </div>
    </div>
    <div class="rr-modal-container">
      <div class="rr-modal-content">
        <div class="rr-header-skeleton">
          {{> skeleton height="100px" width="100px" }}
          <div class="rr-header-skeleton-section">
            {{> skeleton height="16px" width="240px" }}
            {{> skeleton height="16px" width="200px" }}
            {{> skeleton height="16px" width="210px" }}
          </div>
        </div>
          {{#if willRenderStoreList }}
            <div class="rr-input-skeleton">
              <div class="rr-input-skeleton-row">
                {{> skeleton height="25px" width="240px" }}
                {{> skeleton height="25px" width="240px" }}
              </div>
              {{> skeleton height="25px" width="240px" }}
            </div>
        {{/if}}
        <div class="rr-skeleton-divider"></div>
        <div class="rr-skeleton-content">
          {{> skeleton height="24px" width="240px" }}
          {{> skeleton height="24px" width="200px" }}
        </div>
      </div>
      <div class="rr-iti-mobile-dropdown-container" />
    </div>
  </div>
</div>
